Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"boat", " ", "=", " ", 
     RowBox[{"ImplicitRegion", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"BoatCurve", "<", "y", "<", "2"}], "&&", 
        RowBox[{
         RowBox[{"-", "2"}], "<", "x", "<", "2"}], "&&", 
        RowBox[{"0", "<", "y", "<", "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"water", " ", "=", " ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"HeelAngle", " ", "\[Equal]", " ", "90"}], ",", " ", 
       RowBox[{"ImplicitRegion", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"x", ">", "0"}], "&&", 
          RowBox[{
           RowBox[{"-", "2"}], "<", "x", "<", "2"}], "&&", 
          RowBox[{
           RowBox[{"-", "2"}], "<", "y", "<", "4"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "y"}], "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"HeelAngle", "<", "90"}], ",", 
         RowBox[{"ImplicitRegion", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"y", "<", 
             RowBox[{
              RowBox[{
               RowBox[{"Tan", "[", 
                RowBox[{"HeelAngle", " ", "Degree"}], "]"}], "*", 
               RowBox[{"(", "x", ")"}]}], "+", "d"}]}], "&&", 
            RowBox[{
             RowBox[{"-", "2"}], "<", "x", "<", "2"}], "&&", 
            RowBox[{
             RowBox[{"-", "2"}], "<", "y", "<", "4"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}]}], "]"}], ",", " ", 
         RowBox[{"ImplicitRegion", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"y", ">", 
             RowBox[{
              RowBox[{
               RowBox[{"Tan", "[", 
                RowBox[{"HeelAngle", " ", "Degree"}], "]"}], "*", 
               RowBox[{"(", "x", ")"}]}], "+", "d"}]}], "&&", 
            RowBox[{
             RowBox[{"-", "2"}], "<", "x", "<", "2"}], "&&", 
            RowBox[{
             RowBox[{"-", "2"}], "<", "y", "<", "4"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], "]"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"under", " ", "=", " ", 
     RowBox[{"RegionIntersection", "[", 
      RowBox[{"boat", ",", "water"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
